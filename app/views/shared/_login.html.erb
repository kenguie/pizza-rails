<% if current_grubber %>
	<p> Whatddap Grubber?  |  
	<%= link_to 'Sign-Out', logout_path, method: :destroy %> </p>  |  
	<%= link_to 'settings', edit_grubber_path(current_grubber) %> 
<% else %>

<% end %>

<%= form_tag '/sessions', method: :post do %>
	<%= label_tag :email, "Sign in" %>
	<%= text_field_tag :email, nil,  placeholder: "email or mobile" %>
	<%= label_tag :password %>
	<%= password_field_tag :password %>
	<%= submit_tag :login %>
<% end %>